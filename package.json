{
  "name": "communibase-template-data-factory",
  "version": "3.0.0",
  "description": "Helper methods for working with Templates and the Communibase service.",
  "scripts": {
    "test": "node $NODE_DEBUG_OPTION test/runTests.js",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist; babel src --out-dir dist"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kingsquare/communibase-template-data-factory"
  },
  "author": "Kingsquare BV",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/kingsquare/communibase-template-data-factory/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "communibase-connector-js": "^1.0.8",
    "handlebars": "^4.0.12",
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "Communibase": "git+ssh://git@bitbucket.org/communibase/api.git",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bson-stream": "0.0.8",
    "eslint": "^5.9.0",
    "eslint-config-kingsquare": "^5.0.5",
    "grunt": "^1.0.1",
    "grunt-shell": "^2.1.0",
    "kingsquare-handlebars-helpers": "^1.1.8",
    "mocha": "^4.0.1",
    "mongodb": "^2.2.24",
    "prettier": "^1.15.2"
  },
  "engines": {
    "node": "^8"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "kingsquare",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "App": false,
      "Ext": false,
      "config": false
    },
    "rules": {
      "prefer-rest-params": [0]
    }
  }
}
